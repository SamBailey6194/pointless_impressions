ignore = F811
max-line-length = 88
extend-ignore = E203, W503
exclude =
    .git,
    __pycache__,
    build,
    dist,
    migrations,
    env,
    venv,
    .venv,
    .eggs,
    *.egg,
    .tox,
    .venv*/
    .mypy_cache,
    .pytest_cache,
    .vscode
per-file-ignores =
    __init__.py: F401
    */settings/*.py: F403, F405
    */tests/*.py: F403, F405
    */apps.py: F401
import = black